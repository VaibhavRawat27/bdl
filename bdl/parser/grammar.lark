start: statement+

statement: load
         | preview
         | show

load: "load" NAME "from" STRING
preview: "preview" NAME NUMBER?
show: "show" NAME

NAME: /[a-zA-Z_][a-zA-Z0-9_]*/
STRING: ESCAPED_STRING
NUMBER: INT

%import common.INT
%import common.ESCAPED_STRING
%import common.WS
%import common.NEWLINE

%ignore WS
%ignore NEWLINE
